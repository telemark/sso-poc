{
  "name": "sso-poc",
  "version": "2.0.10",
  "description": "SSO poc",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "node standalone.js",
    "dev": "NODE_ENV=development node standalone.js",
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "generate-token": "node utils/generate-test-token.js",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "poc",
    "Telemark fylkeskommune"
  ],
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/sso-poc.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/sso-poc/issues"
  },
  "homepage": "https://github.com/telemark/sso-poc#readme",
  "now": {
    "alias": [
      "sso.poc.t-fk.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "engines": {
      "node": "10.0.0"
    }
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "nsp": "3.2.1",
    "standard": "12.0.0",
    "tap": "12.0.1"
  },
  "dependencies": {
    "axios": "0.18.0",
    "handlebars": "4.0.11",
    "hapi": "16.6.3",
    "hapi-auth-cookie-issamesite-patch": "6.1.2",
    "hapi-auth-jwt2": "7.4.1",
    "hoek": "5.0.4",
    "inert": "4.2.1",
    "jsonwebtoken": "8.3.0",
    "simple-encryptor": "1.4.0",
    "vision": "4.1.1",
    "yar": "8.1.2"
  }
}
